.detail-page {
    width: 100%;
    height: 100%;
    /* overflow-y: hidden; */
}

.house-detail__container {
    position: absolute;
    left: 0;
    top: 0;
    width: 100%;
    height: 100%;
    z-index: 99;
}

.house-detail__bg {
    position: absolute;
    left: 0;
    top: 0;
    width: 0;
    height: 0;
    transform-origin: center center
}

.house-detail__item {
    width: 80%;
    margin: 20rpx auto 0;
}

.house-close {
    position: absolute;
    z-index: 99;
    width: 80rpx;
    left: 0;
    right: 0;
    margin: 0 auto;
}

.house-img {
    width: 80rpx;
    height: 80rpx;
}

.house-item-list {
    display: flex;
    justify-content: space-between;
}

.house-img-item {
    display: block;
    width: 300rpx;
    height: 300rpx; 
    background-color: #eeeeee;
    margin: 0 auto;
}

.house-img__add {
    position: fixed;
    bottom: 0;
    width: 100%;
    height: 100rpx;
    /* background: #ffd845; */
    border-top: 1rpx solid #ffd845;
    background: #ffd7459a;
    color: #333;
    text-align: center;
    line-height: 100rpx;
}


.swiper-tab{
    width: 100%;
    border-bottom: 2rpx solid #ccc;
    text-align: center;
    height: 88rpx;
    line-height: 88rpx;
    display: flex;
    flex-flow: row;
    justify-content: space-between;
}
.swiper-tab-item{
    width: 30%; 
    color:#434343;
}
.active{
    color:#F65959;
    border-bottom: 4rpx solid #F65959;
}

.detail-tab {
    width: 100%;
    min-height: 650rpx;
}

.traffic {
    margin: 20rpx 40rpx;
}

.tab-item {
    display: inline-block;
    font-size: 12pt;
}

.tab-item-right {
    /* display: inline-block; */
    float: right;
    font-size: 10pt;
    color: #333;
}

.map {
    width: 100%;
    height: 600rpx;
}