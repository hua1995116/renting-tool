<view class="home-page">
    <navigator url="../createhouse/createhouse" open-type="navigateTo">
        <view class="house-create__title">
            添加
        </view> 
    </navigator>
     
    <view class="house-create-list">
        <block wx:for="{{listData}}" wx:key="{{index}}">
            <navigator url="../index/index?index={{index}}" open-type="navigateTo">
                <view class="house-create__item" >
                    {{item.location}}
                </view>
            </navigator>
        </block>
        
        <view class="clearfix"></view>
    </view>
    <button class="evaluate-content" hover-class="hover-btn-tran">
        开始评价
    </button>
</view>