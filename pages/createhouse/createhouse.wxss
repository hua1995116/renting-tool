.create-house {
    width: 100%;
    height: 100%;
    background: #f1f1f1;
    overflow: hidden;
}
.create-house__innher {
    padding: 0 50rpx;
}
.create-house__item {
    width: 100%;
    display: flex;
    height: 100rpx;
    box-sizing: border;
    margin: 50rpx 0;
    padding: 0 10rpx;
    background: #fff;
    font-size: 18px;
    border-radius: 5px;
}

.create-label {
    flex-basis: 200rpx;
    display: flex;
    align-items: center;
}

.create-input {
    display: flex;
    flex: 1;
    align-items: center;
}

.create-content {
    flex: 1;
    display: flex;
    align-items: center;
}
.create-date {
    width: 100%;
    height: 50rpx;
}
.create-check__group {
    width: 100%;
}
.create-checkbox {
    width:25%; 
    display: block;
    float: left;
}

.create-submit {
    width: 250rpx;
    height: 250rpx;
    margin-top: 200rpx;
    background: #fed700;
    border-radius: 100%;
    display: flex;
    justify-content: center;
    align-items: center;
    color: #fff;
    font-size: 22px;
    box-shadow: 0 0 5rpx 5rpx #fed700;
    padding: 0;
    border: 0;
    outline: none;
}

.hover-btn-tran {
    background: rgb(12, 226, 41);
}
